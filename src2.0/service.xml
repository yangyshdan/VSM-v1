<?xml version="1.0" encoding="UTF-8"?>
<!-- 说明：通过该配置文件配置储存系统的链接信息 
 		若要增加采集一套存储的信息只需添加一对<storage></storage>即可
 		配置格式如下：
		id:必须唯一，且为正整数，长度不能超过12位；
		type：HDS；
		name：必须唯一，只能包含数字和字母；
		username：存储系统用户；
		password：系统用户密码；
		ctl0_ip：存储系统控制器0的ip地址；
		ctl1_ip：存储系统控制器1的ip地址；
		native_cli_path: 代理目录,默认为：C:\\NavisphereCLI(针对EMC存储)
 		(注意：除ip信息外的字段必填，两个IP至少需要填写一个，建议两个都填写，否则可能导致性能信息有差异)
 -->
<storages>
	<!-- HDS config 
	<storage id="1">
		<type>HDS</type>
		<name>AMS2100</name>
		<username>admin</username>
		<password>password</password>
		<ctl0_ip>192.168.1.1</ctl0_ip>
		<ctl1_ip>192.168.1.2</ctl1_ip>
	</storage>
	-->
	<!-- SVC Info collect 
	<storage id="2">
		<type>SVC</type>
		<name>SVCCollect</name>
		<username>huiming</username>
		<password>password</password>
		<ctl0_ip>192.168.1.127</ctl0_ip>
		<ctl1_ip></ctl1_ip>
	</storage>
	-->
	<!-- EMC Info collect -->
	<storage id="3">
		<type>EMC</type>
		<name>K10</name>
		<username>huiming</username>
		<password>password</password>
		<ctl0_ip>192.168.1.127</ctl0_ip>
		<ctl1_ip></ctl1_ip>
		<native_cli_path></native_cli_path>
	</storage>
	
</storages>